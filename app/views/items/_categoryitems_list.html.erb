<div class="tab-pane active" id="#itemindex">
   <table class="table table-striped table-hover row-clickable">
      <thead>
        <tr >
          <th>Name </th>
          <th>Description </th>
          <th>Contact</th>
          <th>Available</th>
          <th>Additional Info</th>
          <th>Image </th>
        </tr>
      </thead>  
      <tbody class="js-items"> 
           <%= render partial: 'items/item', collection: items %> 
      </tbody> 
   </table>
  
</div> 
  <div id="modal-window" class="modal" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"></div>

 